<Animation>
  <Frame 
    SpriteSheet="Name of the spritesheet for this frame" 
    FrameDelay="Number of milliseconds to wait before moving to the next frame" 
    Width="Width of the frame"
    Height="Height of the frame"
    TLPos="Top left of the frame within the spritesheet of the form 0,0"
    AnimationPeg="Animation peg for the frame of the form 10,10"
    >
    <!--List of all collision data for a frame-->
      <!--Collision data can be Rectangle or Circle. Rectangles with the TLPos and BRPos defined are AABB if TRPos and BLPos are also defined the rectangle is an oriented bounding box-->
      <Collision Type="Rectangle" TLPos="0,0" BRPos="1500.0,100.0"/>
      <Collision Type="Circle" Pos="centerpoint of the circle of the form 0,0" Radius="a float"/>
  </Frame>
</Animation>